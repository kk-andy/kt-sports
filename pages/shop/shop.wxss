/* pages/shop/shop.wxss */
.page-section-spacing{
  margin-top: 60rpx;
}
.scroll-view_H{
  white-space: nowrap;
}
.scroll-view-item{
  height: 300rpx;
}
.scroll-view-item_H{
  display: inline-block;
  width: 100%;
  height: 300rpx;
}
.headNav{
  height: 50px;
  position: fixed;
  padding-left: 40rpx;
  z-index: 1000;
  background: white;
}
.headNavLeft{
  width: 400rpx;
  height: 50px;
  display: flex;
  justify-content: space-between;
  float: left;
 }
.headNavLeftItem{
  width: 100rpx;
  line-height: 50px;
  font-size: 20rpx;
  color: rgb(190, 190, 190);
}
.headNavLeftItemSel{
  width: 100rpx;
  line-height: 50px;
  font-size: 20rpx;
  color:black
}
.headNavRight{
  width: 110rpx;
  float: left;
  height: 50px;
  line-height: 50px;
  font-size: 20rpx;
  overflow: hidden;
  padding-left: 200rpx;
}
.headNavRight image{
  width: 30rpx;
  height: 30rpx;
  margin-top: 17px;
  display: block;
  float: left;
  margin-right: 5rpx;
}
.headNavRight text{
  line-height: 50px;
  margin-top: 0;
}